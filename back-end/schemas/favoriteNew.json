{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/favoriteNew.schema.json",
  "type": "object",
  "properties": {
    "username": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "favorite_id": {
      "type": "integer",
      "minimum": 1
    },
    "type": {
      "type": "string",
      "minLength": 4,
      "maxLength": 10
    }
  },
  "additionalProperties": false,
  "required": [
    "username",
    "favorite_id",
    "type"
  ]
}
